<!DOCTYPE html>
<html>
<head>
  <title>SgUi</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="page">
		<header>
		    <%= render "shared/header" %>
		</header>
		<aside>
		    <%= render "shared/sidebar" %>
		</aside>
		<section>
			<%= yield %>
		</section>
		<header>
		    <%= render "shared/footer" %>
		</header>
	</div>
</body>
</html>
